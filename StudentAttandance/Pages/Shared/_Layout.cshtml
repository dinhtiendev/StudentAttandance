<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <title>@ViewData["Title"] - StudentAttandance</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/StudentAttandance.styles.css" asp-append-version="true" />
    <style>
        .private-information {
            background-color: #5cb85c;
        }
    </style>
</head>
<body>
    <div class="container">
        <header class="mx-auto text-center">
            <img src="./images/Logo_Đại_học_FPT.png" alt="Logo" width="300px" height="150px">
        </header>
        @RenderBody()
        <footer>
            <p class="text-center my-2"> <span class="fw-bold">Mọi góp ý, thắc mắc xin liên hệ:</span> Email: <span><a
                        class="text-decoration-none" href="#">admin@fe.edu.vn</a></span>. Điện thoại:
                <span class="fw-bold">(+84) 123.456.789</span>
            </p>
            <p class="text-center">
                © Powered by <span><a class="name-university text-decoration-none" href="">FPT University</a></span>
            </p>
        </footer>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>